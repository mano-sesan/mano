{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "PACKAGE_ID": "com.sesan.mano.expo",
        "EXPO_PUBLIC_MANO_DOWNLOAD_URL": "https://mano.sesan.fr/download-expo"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": false,
      "environment": "production",
      "env": {
        "PACKAGE_ID": "com.sesan.mano",
        "EXPO_PUBLIC_MANO_DOWNLOAD_URL": "https://mano.sesan.fr/download"
      },
      "android": {
        "buildType": "apk",
        "credentialsSource": "remote"
      }
    },
    "production-niort": {
      "extends": "production",
      "env": {
        "PACKAGE_ID": "com.sesan.mano.niort",
        "EXPO_PUBLIC_MANO_DOWNLOAD_URL": "https://mano.sesan.fr/download-niort"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
