{
  "name": "mano",
  "version": "3.11.0",
  "private": true,
  "scripts": {
    "get-ip": "./get-ip.sh",
    "android": "npm run get-ip && react-native run-android",
    "ios": "npm run get-ip && react-native run-ios",
    "start": "react-native start --reset-cache",
    "test": "jest --watch",
    "lint": "eslint .",
    "detox-build": "echo -p \"building NOWWW\" && detox build --configuration android.debug",
    "detox-test": "echo -p \"please npm run start NOWWW\" && detox test ./__tests__ --reuse --configuration android.debug",
    "detox-build-and-test": "npm run detox-build && npm run detox-test",
    "android-clean-start": "npm run clean:android && react-native run-android",
    "postinstall": "npx jetify && npx patch-package",
    "clean:android": "cd android && ./gradlew clean && cd ../ && adb uninstall com.sesan.mano",
    "build:android": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ../",
    "build:android-apk": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ../",
    "build:android-apk-preproduction": "echo -p \"please update either .env.production with the preproduction values until we found a better solution ðŸ¥¸\" && cd android && ./gradlew clean && ./gradlew assembleRelease && cd ../",
    "deploy:android": "./upload.sh",
    "build-and-deploy-preprod:android": "npm run build:android-apk && ./upload-preprod.sh",
    "update-mobile-app-version": "node ./update-mobile-app-version.js",
    "publish-release-to-github": "node ./publish-release-to-github.js"
  },
