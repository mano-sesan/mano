{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:default",
    "sql:default",
    "sql:allow-load",
    "sql:allow-execute",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://api-mano.sesan.fr",
          "credentials": true
        },
        {
          "url": "http://localhost:4145",
          "credentials": true
        }
      ]
    },
    "http:default"
  ]
}